    "react-dom": "^18.1.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.2.0",
    "@types/node": "^17.0.33",
    "@types/react": "^18.0.9",
    "autoprefixer": "^10.4.2",
    "cypress": "^9.6.1",
    "eslint": "8.9.0",
    "eslint-config-next": "12.0.10",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^28.1.0",
    "jest-mock-extended": "^2.0.6",
    "node-mocks-http": "^1.11.0",
    "postcss": "^8.4.6",
    "prettier": "2.5.1",
