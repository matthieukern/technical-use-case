  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "dotenv -e .env.test jest --watch",
    "cypress": "dotenv -e .env.test npm run dev & wait-on http://localhost:3000 && dotenv -e .env.test cypress open",
